{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["google"],


  "ecmaFeatures": {
    "jsx": true
  },

  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "indent": "off",
    "object-curly-spacing": "off",
    "operator-linebreak": "off",
    "require-jsdoc": "off",
    "no-unused-vars": "warn"
  }
}
